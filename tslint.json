{
  "rulesDirectory": [],
  "rules": {
    "max-line-length": {
      "options": [110]
    },
    "new-parens": true,
    "no-unused-variable": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    },
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "always"
        }
      }
    ]
  },
  "jsRules": {
    "max-line-length": {
      "options": [110]
    }
  }
}
